{
    "builds": [
      {
        "src": "auction-server/cmd/auction/main.go",
        "use": "@vercel/go"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "auction-server/cmd/auction/main.go"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  