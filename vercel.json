{
  "builds": [
    {
      "src": "api/main.go",
      "use": "@vercel/go"
    },
    {
      "src": "auction/public/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.go"
    },
    {
      "src": "/(.*)",
      "dest": "/auction/public/$1"
    }
  ]
}
