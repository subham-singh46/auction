{
  "builds": [
    {
      "src": "api/main.go",
      "use": "@vercel/go"
    },
    {
      "src": "auction/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "auction/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.go"
    },
    {
      "src": "/(.*)",
      "dest": "auction/build/$1"
    }
  ]
}
